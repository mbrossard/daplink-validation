        ],
        "static_memory_defines": false,
        "macros_add": [
            "TARGET_MCU_NRF52833",
            "NRFX_RNG_ENABLED=1",
            "RNG_ENABLED=1",
            "NRF_QUEUE_ENABLED=1"
        ],
        "features_remove": [
            "CRYPTOCELL310"
        ],
        "macros_remove": [
